<section id="ignorable">
    <title>anonymous subunit object <code>"kind" : "ignorable"</code></title>
    <para>Part of the extracted content that is not included in a segment (and therefore not
        translatable). For example tools can use <code>&lt;ignorable&gt;</code> to store the white
        space and/or codes that are between two segments.</para>
    <para><emphasis>Type:</emphasis> object.</para>
    
    <para><emphasis>Properties:</emphasis></para>
    <simplelist>
        <member>-
                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"id"</code>, a<?oxy_custom_end?>
            <olink targetdoc="../../attributes/id.xml" targetptr="id"><code>id</code></olink>
            <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?>string with NMTOKEN
            value<?oxy_custom_end?>, <glossterm>optional</glossterm></member>
        <member>-
                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"kind"</code>, a<?oxy_custom_end?>
            <olink targetdoc="../../attributes/kind.xml" targetptr="kind"><code>kind</code></olink>
            <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?>string<?oxy_custom_end?>,
                <glossterm>required</glossterm><?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?>
            with fixed value <code>"ignorable"</code><?oxy_custom_end?></member>
        <member>- <code>"sourceXmlLang"</code>, a <olink targetdoc="../../attributes/xml_lang.xml" targetptr="xml_lang"><code>"sourceXmlLang"</code></olink> string, <glossterm>optional</glossterm> with a
            [BCP 47] tag value</member>
        <member>- <code>"targetXmlLang"</code>, a <olink targetdoc="../../attributes/xml_lang.xml" targetptr="xml_lang"><code>"targetXmlLang"</code></olink> string, <glossterm>optional</glossterm> with a
            [BCP 47] tag value</member>
        <member>- <code>"targetOrder"</code>, a <olink targetdoc="../../attributes/order.xml"
                targetptr="order"><code>"targetOrder"</code></olink> number,
                <glossterm>optional</glossterm> with a positive integer value</member>
        <member>-
                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"source"</code>,
            a non-empty array of
                <?oxy_custom_end?><?oxy_custom_start type="oxy_content_highlight" color="255,255,0"?><code>"element"</code>
            objects<?oxy_custom_end?><?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?>,
                <glossterm>required</glossterm><?oxy_custom_end?></member>
        <member>-
                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"target"</code>,
            a non-empty array of
                <?oxy_custom_end?><?oxy_custom_start type="oxy_content_highlight" color="255,255,0"?><code>"element"</code>
            objects<?oxy_custom_end?><?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?>,
                <glossterm>optional</glossterm></member><?oxy_custom_end?>
    </simplelist>
    
